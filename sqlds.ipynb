   "source": [
    "python program to check whether a string is palindrome or not\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "it is palindrome\n"
     ]
    }
   ],
   "source": [
    "mystr = \"NooN\"\n",
    "\n",
    "mystr=mystr.lower()\n",
    "\n",
    "revstr=reversed(mystr)\n",
    "\n",
    "#CHEK IF THE STRING IS EQUAL TO ITS REVERSE\n",
    "if list(mystr)==list(revstr):\n",
    "    print(\"it is palindrome\")\n",
    "else:\n",
    "    print(\"it is not a palindrome\")\n"
   ]
  },
